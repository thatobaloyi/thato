{
  "version": 2,
  "builds":[
    {
      "src": "*.js", "use": "@vercel/node"
    }
  ],
  "routes":[
    {
      "src": "/api/projects",
      "dest": "https://thato-portfolio-frontend.vercel.app/",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
