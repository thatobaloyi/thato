{
  "version": 2,
  "builds":[
    {
      "src": "*.js", "use": "@vercel/node"
    }
  ],
  "routes":[
    {
      "src": "/api/(.*)",
      "dest": "https://thato-server.vercel.app/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src":"/(.*)",
      "dest":"/index.html"
    }
  ]
}
