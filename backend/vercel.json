{
  "version":2,
  "builds":[
    {"src": "*.js", "use": "@vercel/node"}
  ],
 "routes": [
    {
      "src": "/projects",
      "dest": "./index.js"
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
